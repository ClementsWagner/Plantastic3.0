mvn io.quarkus:quarkus-maven-plugin:1.9.2.Final:create \
    -DprojectGroupId=at.htl.super-duper-plantastic.core \
    -DprojectArtifactId=core-sensor \
    -DclassName="at.htl.superduperplantastic.coresensor.rest.SensorEndpoint" \
    -Dpath="v1/sensors" \
    -Dextensions="resteasy-jsonb"

mvn io.quarkus:quarkus-maven-plugin:1.9.2.Final:create \
    -DprojectGroupId=at.htl.super-duper-plantastic \
    -DprojectArtifactId=rest-sensor \
    -DclassName="at.htl.superduperplantastic.sensor.rest.SensorEndpoint" \
    -Dpath="api/v1/sensors" \
    -Dextensions="resteasy-jsonb"
cd rest-kotlin-quickstart
